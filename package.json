{
  "name": "@clau-org/ui-core",
  "type": "module",
  "version": "0.0.13",
  "main": "./nuxt.config.ts",
  "scripts": {
    "predev": "npm run prepare",
    "dev": "nuxi dev .playground",
    "test": "vitest run",
    "test:dev": "vitest",
    "coverage": "vitest run --coverage",
    "build": "NITRO_PRESET=deno nuxt build .playground",
    "generate": "nuxt generate .playground",
    "preview": "cd .playground/.output/ && deno run -A --watch server/index.ts",
    "prepare": "nuxt prepare .playground",
    "format": "npx prettier --write .",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.3.2",
    "@vitest/coverage-c8": "^0.29.7",
    "nuxt": "^3.2.3",
    "typescript": "^4.9.3",
    "vitest": "^0.29.7"
  },
  "dependencies": {
    "@formkit/auto-animate": "^1.0.0-beta.6",
    "@headlessui/vue": "^1.7.11",
    "@heroicons/vue": "^2.0.16",
    "@nuxtjs/device": "^3.1.0",
    "@nuxtjs/partytown": "^1.3.0",
    "@nuxtjs/tailwindcss": "^6.4.1",
    "@pinia-plugin-persistedstate/nuxt": "^1.1.1",
    "@pinia/nuxt": "^0.4.7",
    "@sidebase/nuxt-pdf": "^0.1.1",
    "@vue-hero-icons/outline": "^1.7.2",
    "@vue-hero-icons/solid": "^1.7.2",
    "@vue-macros/nuxt": "^1.2.3",
    "@vueuse/core": "^9.13.0",
    "@vueuse/nuxt": "^9.13.0",
    "magic-regexp": "^0.6.3",
    "nuxt": "^3.2.3",
    "nuxt-headlessui": "^1.1.1",
    "nuxt-icon": "^0.3.2",
    "nuxt-schema-org": "^2.1.2",
    "nuxt-swiper": "^1.0.1",
    "nuxt-typed-router": "^3.0.6",
    "nuxt-viewport": "^2.0.3",
    "pinia": "^2.0.32",
    "vue3-json-source-viewer": "^2.4.2"
  },
  "publishConfig": {
    "@clau-org:registry": "https://npm.pkg.github.com"
  }
}
