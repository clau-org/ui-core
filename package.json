{
  "name": "@clau-org/ui-core",
  "type": "module",
  "version": "0.0.9",
  "main": "./nuxt.config.ts",
  "scripts": {
    "predev": "npm run prepare",
    "dev": "nuxi dev .playground",
    "test": "vitest run",
    "test:dev": "vitest",
    "coverage": "vitest run --coverage",
    "build": "NITRO_PRESET=deno nuxt build .playground",
    "generate": "nuxt generate .playground",
    "preview": "cd .playground/.output/ && deno run -A --watch server/index.ts",
    "prepare": "nuxt prepare .playground",
    "format": "npx prettier --write .",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.3.2",
    "@vitest/coverage-c8": "^0.29.7",
    "nuxt": "^3.2.3",
    "typescript": "^4.9.3",
    "vitest": "^0.29.7"
  },
  "dependencies": {
    "@clau-org/ui-configuration": "^0.0.11",
    "@sidebase/nuxt-pdf": "^0.1.1",
    "vue3-json-source-viewer": "^2.4.2"
  },
  "publishConfig": {
    "@clau-org:registry": "https://npm.pkg.github.com"
  }
}
